<h1 class="session_new">入館</h1>
  <div class ="row">
    <div class=" col-md-6 col-md-offset-3">
      <%= form_with(scope: :session, url:login_path, local:true) do |f|%>

        <%= f.label :email, class:"label-login"%>
        <%= f.email_field :email, class: "form-control"%>

        <%=f.label :password, class:"label-label"%>
        <%=f.password_field :password, class: "form-control"%>

        <%= f.submit "入館", class: "btn btn-primary btn-block btn-login"%>
      <%end%>
      <p>ユーザー登録はこちらから</p>
    </div>
  </div>
